<table class="table table-hover" id="mytable">
  <thead>
    <tr>
      <th scope="col">Product</th>
      <th scope="col">Category</th>
      <th scope="col">Price</th>
      <th scope="col">Qty</th>
      <th scope="col">Sales</th>
    </tr>
  </thead>
  {{#each productData2}}
  <tbody>
    <tr>
      <td>{{this.Name}}</td>
      <td>{{category.[0].Name}}</td> 
      <td>{{this.Price}}</td>
      <td>{{this.count}}</td>
      <td>{{this.sum}}</td>
   
    </tr>
  </tbody>
  {{/each}}
</table>
